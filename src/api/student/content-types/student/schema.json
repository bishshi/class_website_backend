{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Sex": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Male",
        "Female"
      ]
    },
    "Photo": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "Birthday": {
      "type": "date"
    },
    "Phone": {
      "type": "biginteger"
    },
    "Email": {
      "type": "email"
    },
    "Introduction": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultMarkdown"
      }
    },
    "relatedArticle": {
      "type": "text"
    }
  }
}
